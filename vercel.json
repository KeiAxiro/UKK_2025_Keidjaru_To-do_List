{
  "builds": [
    {
      "src": "/src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/src/(.*)",
      "dest": "/src/app.ta"
    },
    {
      "src": "/src/(.+)",
      "dest": "/src/public/$1"
    }
  ]
}
